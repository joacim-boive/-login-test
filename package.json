{
    "name": "ecster-login",
    "version": "0.0.1",
    "private": true,
    "description": "ecster-login created by Rekit.",
    "keywords": [
        "rekit"
    ],
    "rekit": {
        "devPort": 6075,
        "studioPort": 6076,
        "buildPort": 6077,
        "restPort": 6078,
        "plugins": [],
        "css": "sass"
    },
    "prettier": {
        "printWidth": 120,
        "bracketSpacing": true,
        "trailingComma": "es5",
        "singleQuote": true,
        "semi": true,
        "useTabs": false,
        "tabWidth": 4
    },
    "browserslist": [
        "last 2 versions",
        "ie 11",
        "not ie < 11",
        "not ie_mob 10-99",
        "not bb 7-99"
    ],
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
        },
        "testPathIgnorePatterns": [
            "/tests/"
        ],
        "collectCoverageFrom": [
            "<rootDir>/src/**/*test.(js|jsx)",
            "!<rootDir>/src/knockout-components/**/*.(js|jsx)"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "testResultsProcessor": "jest-teamcity-reporter"
    },
    "scripts": {
        "clear:caches": "rimraf node_modules/.cache src/node_modules/.cache",
        "pre:build:clear_build": "rimraf build",
        "prebuild": "npm-run-all pre:build:*",
        "prebuild:dev": "npm-run-all pre:build:clear_build",
        "help": "node ./src/lib/node-start.js --help",
        "prettier": "prettier",
        "build": "node ./compile.js --prod --port 8081",
        "build:ci": "node ./compile.js --prod --ci",
        "build:vendor": "node ./compile.js --vendor",
        "build:dll": "node ./compile.js --vendor",
        "build:vendor:debug": "node --inspect-brk ./compile.js --debug --vendor",
        "build:dll:debug": "node --inspect-brk ./compile.js --debug --vendor",
        "start": "nodemon ./compile.js --watch ./src/lib --dev",
        "start:debug": "nodemon --inspect-brk ./compile.js --watch ./src/lib/node-start.js --debug --dev",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "record": "node ./tools/server.js -record",
        "mock": "node ./tools/server.js -mock",
        "cordova": "cd cordova && cordova run browser",
        "android": "cd cordova && cordova run android",
        "ios": "cd cordova && cordova run ios"
    },
    "appConfig": {
        "title": "Ecster - Inloggat X",
        "description": "VÃ¤lkommen till Ecster",
        "buildCatalog": "build",
        "sourceCatalog": "src",
        "isDontIndexThisPage": false,
        "isGoogleFonts": false,
        "isInjectReferences": true,
        "isOptimizeCSS": false,
        "cssWhiteList": [],
        "language": "sv",
        "googleAnalytics": {
            "id": "UA-123456-1",
            "script": "ga/analytics.js"
        },
        "entries": [
            "index.js",
            "styles/index.scss"
        ],
        "copy": [
            {
                "from": "i18n",
                "to": "static/i18n"
            },
            {
                "from": "ga",
                "to": "ga"
            }
        ],
        "template": "template.ejs",
        "favicon": "favicon.png",
        "dll": [
            "react",
            "react-dom",
            "react-redux",
            "react-router-dom",
            "react-router-redux",
            "redux",
            "redux-thunk",
            "moment",
            "classnames",
            "lazysizes/plugins/respimg/ls.respimg.min",
            "lazysizes/plugins/bgset/ls.bgset.min",
            "lazysizes/plugins/rias/ls.rias.min",
            "lazysizes",
            "jquery-param",
            "pikaday",
            "mobile-detect",
            "qwest"
        ],
        "proxies": [
            {
                "contexts": [
                    "/rest/"
                ],
                "target": "https://secure5.ft.ecster.se",
                "headers": [
                    [
                        "X-ECSTER-Origin",
                        "mypages"
                    ],
                    [
                        "Content-Type",
                        "application/json"
                    ]
                ]
            }
        ]
    },
    "devDependencies": {
        "@ecster/ecster-dev-server": "^1.0.13",
        "ajv": "^6.5.2",
        "argparse": "^1.0.7",
        "axios": "^0.17.0",
        "body-parser": "^1.18.3",
        "chai": "^4.1.2",
        "colors": "^1.3.1",
        "core-js": "^2.5.7",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.3.4",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-import-resolver-babel-module": "^4.0.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-jsx-a11y": "^6.1.1",
        "eslint-plugin-prettier": "^2.6.2",
        "eslint-plugin-react": "^7.10.0",
        "express": "^4.16.3",
        "express-history-api-fallback": "^2.2.1",
        "history": "^4.7.2",
        "jest": "^23.4.1",
        "jest-teamcity-reporter": "^0.9.0",
        "jsdom": "^11.11.0",
        "lodash": "^4.17.10",
        "mocha": "^4.1.0",
        "nock": "^9.4.3",
        "normalize-scss": "^7.0.1",
        "npm-install-webpack-plugin": "^4.0.5",
        "npm-run": "^4.1.2",
        "nyc": "^11.9.0",
        "prop-types": "^15.6.2",
        "react-hot-loader": "^4.3.2",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.5.3",
        "rekit-core": "^2.3.4",
        "rekit-studio": "^2.4.1",
        "request": "^2.87.0",
        "sinon": "^4.5.0",
        "why-did-you-update": "^0.1.1"
    },
    "dependencies": {
        "@ecster/ecster-components": "^1.0.349",
        "@ecster/ecster-i18n": "^3.0.63",
        "@ecster/ecster-net": "^1.0.74",
        "@ecster/ecster-styles": "^2.0.192",
        "@ecster/ecster-util": "^2.0.71",
        "@ecster/ecster-webpack": "^2.0.132",
        "@types/http-proxy-middleware": "^0.17.5",
        "classnames": "^2.2.6",
        "features": "^0.1.0",
        "lazysizes": "^4.0.4",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-grid-layout": "^0.16.6",
        "react-media": "^1.8.0",
        "react-redux": "^5.0.5",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "5.0.0-alpha.9",
        "redux": "^3.7.2",
        "redux-thunk": "^2.3.0"
    }
}
