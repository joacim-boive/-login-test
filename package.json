{
    "name": "ecster-login",
    "version": "0.0.1",
    "private": true,
    "description": "ecster-login created by Rekit.",
    "keywords": [
        "rekit"
    ],
    "rekit": {
        "devPort": 6075,
        "studioPort": 6076,
        "buildPort": 6077,
        "restPort": 6078,
        "plugins": [],
        "css": "sass"
    },
    "prettier": {
        "printWidth": 120,
        "bracketSpacing": true,
        "trailingComma": "es5",
        "singleQuote": true,
        "semi": true,
        "useTabs": false,
        "tabWidth": 4
    },
    "browserslist": [
        "last 2 versions",
        "ie 11",
        "not ie < 11",
        "not ie_mob 10-99",
        "not bb 7-99"
    ],
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
        },
        "testPathIgnorePatterns": [
            "/tests/"
        ],
        "collectCoverageFrom": [
            "<rootDir>/src/**/*test.(js|jsx)",
            "!<rootDir>/src/knockout-components/**/*.(js|jsx)"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "testResultsProcessor": "jest-teamcity-reporter"
    },
    "scripts": {
        "start": "node ./tools/server.js",
        "record": "node ./tools/server.js -record",
        "mock": "node ./tools/server.js -mock",
        "build": "node ./tools/build.js && node webpack-after-build.js",
        "build-debug": "node ./tools/build.js --debug DEBUG && node webpack-after-build.js",
        "test": "jest",
        "cordova": "cd cordova && cordova run browser",
        "android": "cd cordova && cordova run android",
        "ios": "cd cordova && cordova run ios"
    },
    "appConfig": {
        "title": "Ecster - Inloggat X",
        "description": "VÃ¤lkommen till Ecster",
        "buildCatalog": "build",
        "sourceCatalog": "src",
        "isDontIndexThisPage": false,
        "isGoogleFonts": false,
        "isInjectReferences": true,
        "isOptimizeCSS": false,
        "cssWhiteList": [],
        "language": "sv",
        "googleAnalytics": {
            "id": "UA-123456-1",
            "script": "ga/analytics.js"
        },
        "entries": [
            "index.js",
            "styles/index.scss"
        ],
        "copy": [
            {
                "from": "i18n",
                "to": "static/i18n"
            },
            {
                "from": "ga",
                "to": "ga"
            }
        ],
        "template": "template.ejs",
        "favicon": "favicon.png"
    },
    "devDependencies": {
        "@ecster/ecster-dev-server": "^1.0.12",
        "argparse": "^1.0.7",
        "axios": "^0.17.0",
        "body-parser": "^1.18.3",
        "chai": "^4.1.2",
        "core-js": "^2.5.7",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.3.4",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-import-resolver-babel-module": "^4.0.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.9.1",
        "express": "^4.16.3",
        "express-history-api-fallback": "^2.2.1",
        "history": "^4.7.2",
        "jest": "^23.1.0",
        "jsdom": "^11.11.0",
        "lodash": "^4.17.10",
        "mocha": "^4.1.0",
        "nock": "^9.3.3",
        "npm-run": "^4.1.2",
        "nyc": "^11.9.0",
        "prop-types": "^15.5.10",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-hot-loader": "^4.3.2",
        "react-redux": "^5.0.5",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "5.0.0-alpha.9",
        "redux": "^3.7.2",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.5.3",
        "redux-thunk": "^2.3.0",
        "rekit-core": "^2.3.3",
        "rekit-studio": "^2.4.0",
        "request": "^2.87.0",
        "shelljs": "^0.8.2",
        "sinon": "^4.5.0"
    },
    "dependencies": {
        "@babel/polyfill": "^7.0.0-beta.51",
        "@ecster/ecster-components": "^1.0.337",
        "@ecster/ecster-i18n": "^3.0.63",
        "@ecster/ecster-net": "^1.0.74",
        "@ecster/ecster-styles": "^2.0.192",
        "@ecster/ecster-util": "^2.0.67",
        "@ecster/ecster-webpack": "^2.0.104",
        "ajv": "^6.5.1",
        "classnames": "^2.2.6",
        "features": "^0.1.0",
        "jest-teamcity-reporter": "^0.9.0",
        "lazysizes": "^4.0.4",
        "normalize-scss": "^7.0.1",
        "npm-install-webpack-plugin": "^4.0.5",
        "react-grid-layout": "^0.16.6",
        "react-media": "^1.8.0",
        "react-router": "^4.3.1",
        "why-did-you-update": "^0.1.1"
    }
}
